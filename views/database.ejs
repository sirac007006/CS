            </select>

            <!-- StatTrak/Souvenir Filter -->
            <label for="typeFilter">StatTrak/Souvenir</label>
            <select id="typeFilter">
                <option value="">Select Type</option>
                <option value="StatTrak">StatTrak</option>
                <option value="Non-StatTrak">Non-StatTrak</option>
                <option value="Souvenir">Souvenir</option>
                <option value="Non-Souvenir">Non-Souvenir</option>
            </select>

            <!-- Submit Button -->
            <button type="button" id="applyFilters">Apply Filters</button>
        </div>
    </div>

    <script>
        // JavaScript for Modal Behavior
        const searchModal = document.getElementById('searchModal');
        const openSearchModal = document.getElementById('openSearchModal');
        const closeSearchModal = document.getElementById('closeSearchModal');
        const modal = document.getElementById("skinDetailsModal");
        const closeModal = document.getElementById("closeSkinModal");
        const skinImage = document.getElementById("skinImage");
        const patternWear = document.getElementById("patternWear");
        const floatValue = document.getElementById("floatValue");

        openSearchModal.onclick = function () {
            searchModal.style.display = 'flex';
        };

        closeSearchModal.onclick = function () {
            searchModal.style.display = 'none';
        };

        window.onclick = function (event) {
            if (event.target == searchModal) {
                searchModal.style.display = 'none';
            }
        };
        document.querySelectorAll(".skin-card").forEach(card => {
            card.addEventListener("click", () => {
                const skinName = card.getAttribute("data-skin-name");
                const skinPrice = card.getAttribute("data-price");
                const skinImg = card.getAttribute("data-skin-image");

                // Fill modal with data
                skinImage.src = skinImg;
                patternWear.textContent = `Pattern Wear: ${(Math.random() * 1).toFixed(3)}`;
                floatValue.textContent = `Float: ${(Math.random() * 1).toFixed(5)}`;

                // Show modal
                modal.style.display = "block";

                // Generate sales graph
                generateSalesGraph();
            });
        });

        // Close modal
        closeModal.addEventListener("click", () => {
            modal.style.display = "none";
        });

        // Close modal on click outside
        window.addEventListener("click", (e) => {
            if (e.target === modal) {
                modal.style.display = "none";
            }
        });

        // Generate random sales data for graph
        function generateSalesGraph() {
            const ctx = document.getElementById("salesGraph").getContext("2d");
            new Chart(ctx, {
                type: "line",
                data: {
                    labels: ["1 week ago", "5 days ago", "3 days ago", "1 day ago", "Today"],
                    datasets: [{
                        label: "Price ($)",
                        data: [
                            Math.floor(Math.random() * 100) + 500,
                            Math.floor(Math.random() * 100) + 550,
                            Math.floor(Math.random() * 100) + 600,
                            Math.floor(Math.random() * 100) + 650,
                            Math.floor(Math.random() * 100) + 700
                        ],
                        borderColor: "blue",
                        borderWidth: 2
                    }]
                }
            });
        }

        // Additional functionality like applying filters can be added here
    </script>
</body>

</html>
