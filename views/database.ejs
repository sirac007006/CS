        /* Skin Details Modal Styles */
        .skin-details-modal {
            width: 90%;
            max-width: 1200px;
            height: 80vh;
            overflow-y: auto;
            padding: 25px;
        }

        .skin-modal-grid {
            display: grid;
            grid-template-columns: 1fr 2fr 1fr;
            gap: 20px;
            height: 100%;
        }

        /* Left Section - Sales Graph */
        .skin-modal-left {
            display: flex;
            flex-direction: column;
        }

        .sales-graph-container {
            background: var(--bg-card);
            border: 1px solid var(--border-color);
            border-radius: 8px;
            padding: 10px;
            height: 250px;
            margin-bottom: 20px;
        }

        .recent-sales-container {
            background: var(--bg-card);
            border: 1px solid var(--border-color);
            border-radius: 8px;
            padding: 10px;
            flex-grow: 1;
            overflow-y: auto;
        }

        .recent-sales-table {
            width: 100%;
            border-collapse: collapse;
        }

        .recent-sales-table th, .recent-sales-table td {
            padding: 8px;
            text-align: left;
            border-bottom: 1px solid var(--border-color);
        }

        /* Center Section - Skin Image and Details */
        .skin-modal-center {
            display: flex;
            flex-direction: column;
        }

        .seller-profile {
            display: flex;
            align-items: center;
            gap: 10px;
            margin-bottom: 15px;
        }

        .seller-avatar {
            width: 40px;
            height: 40px;
            border-radius: 50%;
            object-fit: cover;
        }

        .seller-name {
            font-weight: 500;
        }

        .skin-image-container {
            background: var(--bg-darker);
            border-radius: 8px;
            overflow: hidden;
            height: 300px;
            display: flex;
            justify-content: center;
            align-items: center;
            margin-bottom: 20px;
        }

        .skin-image-large {
            max-width: 100%;
            max-height: 100%;
            object-fit: contain;
        }

