    <div class="database-content">
        <div class="content-header">
            <h2 class="content-title">Browse and buy all CS2 & CS:GO skins</h2>
            <button class="search-button" id="openSearchModal">Filtered Search</button>
        </div>

        <div class="skins-grid">
            <div class="skin-card" data-skin-name="Bayonet | Doppler" data-price-range="$632.99 - $651.86" data-skin-type="Covert Knife" data-case-info="3 containers">
