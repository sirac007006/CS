        }

        .float-bar {
            height: 10px;
            background: linear-gradient(to right, #3498db, #2ecc71, #f1c40f, #e67e22, #e74c3c);
            border-radius: 5px;
            position: relative;
            margin: 10px 0;
        }

        .float-indicator {
            position: absolute;
            width: 4px;
            height: 16px;
            background-color: white;
            top: -3px;
            border-radius: 2px;
        }

        .float-ranges, .float-categories {
            display: flex;
            justify-content: space-between;
            font-size: 0.7rem;
            color: var(--text-secondary);
        }

        .pattern-container {
            margin-top: 10px;
        }

        .stickers-container {
            margin-bottom: 20px;
        }

        .stickers-grid {
            display: flex;
            gap: 10px;
            margin-top: 10px;
        }

        .sticker-item {
            width: 60px;
            height: 60px;
            background: var(--bg-darker);
            border-radius: 4px;
            overflow: hidden;
        }

        .sticker-img {
            width: 100%;
            height: 100%;
            object-fit: contain;
        }

        .action-buttons {
            display: flex;
            gap: 15px;
        }

        .buy-now-btn, .add-cart-btn {
            flex: 1;
